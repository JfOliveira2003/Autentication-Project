@model System.Collections.Generic.List<AccountClient>;
@{
    ViewData["Title"] = "Accounts";
}

<h1>Client accounts administration</h1>
<div class="box">

    <a asp-action="Create">Add a new Account</a>
</div>

<div class="box-2">
    <ul>
        @foreach(var item in Model){
            <li>
                @Html.DisplayFor(modelItem => item.Holder)
                @Html.DisplayFor(modelitem => item.Number)
            </li>
        }
    </ul>
</div>